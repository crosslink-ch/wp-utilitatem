{
	"name": "crosslink-ch/wp-utilitatem",
	"version": "0.3.5",
	"type": "library",
	"authors": [],
	"autoload": {
		"psr-4": {
			"WpUtm\\": "src/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"build/",
			"wp-utilitatem.php",
			"package.json",
			"*.xml",
			"*.zip",
			"WordPress-master/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WpUtm\\Plugin\\": "tests/plugin",
			"WpUtm\\Test\\": "tests/phpunit"
		}
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.11",
		"wp-coding-standards/wpcs": "^3.1",
		"yoast/phpunit-polyfills": "^3.0",
		"wp-phpunit/wp-phpunit": "^6.7"
	},
	"require": {
		"php-di/php-di": "^7.0"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
